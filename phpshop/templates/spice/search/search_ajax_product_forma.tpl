
<div class='news-list'>
    <a href="/shop/UID_@productUid@.html">
        <div class="template-pad media">
            <div class="media-left">
                <img src="@productImg@" alt="@productName@" width='64' class="media-object img-rounded" onerror="NoFoto(this,'@pathTemplate@')">  
            </div>
            <div class="media-body">
                <h4 class="media-heading">@productName@</h4>
                <span class="btn btn-default">@productPrice@ <span class="rubznak">@productValutaName@</span></span>
            </div>
        </div>
    </a>
</div>
